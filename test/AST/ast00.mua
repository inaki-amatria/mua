function foo(bar, baz)
  qux = bar + baz
  mux = qux * 2 - 3 / bar
  return mux
end

function bar(baz)
  qux = 1
  mux = mux + 1
  qux = qux * 1
  mux = mux + 1
  qux = qux * 1
  mux = mux + 1
  return qux
end

function baz()
  10
  qux = foo(5, 10)
  mux = bar(5)
  fux = qux + mux * qux / 2
  return fux
end

